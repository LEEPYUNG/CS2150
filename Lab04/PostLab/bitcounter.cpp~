#include <iostream>
#include <string>
using namespace std;


int bitcounter(int n){
  if ( n  == 0 ) {
    return 0;
  }
  else if( n == 1 ){
    return 1;
  }
  else if(n%2 == 0 && n != 0){    
    return bitcounter(n/2);
  }
  else if(n%2 != 0 && n != 1){
    return 1 + bitcoutner(floor(n/2)); 
  }


}

