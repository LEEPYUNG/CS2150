#include "vertex.h"
#include <string>
using namespace std;

vertex::vertex(string name) {
  indegree = 0;
  string courseName = name;
}

vertex::~vertex() {
  //nothing yet.
}

void vertex::increaseIndegree(){
  indegree++;
}

void vertex::decreaseIndegree(){
  indegree--;
}

string vertex::getName(){
  return courseName;
}


			 
