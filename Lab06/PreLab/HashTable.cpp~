#ifndef HASHTABLE_H
#define HASHTABLE_H

#include <vector>
using namespace std;

class HashTable {
public:
    HashTable();                     // Constructor
       
    int hashFunction(string word);  // Deletes tree/frees memory
    void insert(string word);       // Gets data from user
    void find(string word);         // Puts value in tree


};

#endif
